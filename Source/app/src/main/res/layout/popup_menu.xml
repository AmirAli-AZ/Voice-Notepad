<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/lin_popup"
		android:layout_width="195dp"
		android:layout_height="wrap_content"
		android:layout_marginLeft="10dp"
		android:layout_marginTop="13dp"
		android:layout_marginRight="6dp"
		android:layout_marginBottom="10dp"
		android:background="#303030"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/lin_import"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img1"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/file_import"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt1"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Import"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/div1"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/lin_search"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img2"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/search1"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt2"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Search"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/div2"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/lin_copy"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img3"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/content_copy"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt3"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Copy"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/div3"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/lin_paste"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img4"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/content_paste1"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt4"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Paste"
				android:textSize="14sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/div4"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/lin_U"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img5"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/upper_case"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt5"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Upper Case"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/div5"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/lin_L"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img6"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/lower_case"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt6"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Lower Case"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/div6"
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/lin_Prettify"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/img7"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:src="@drawable/prettify"
				android:scaleType="fitCenter"/>
			<TextView
				android:id="@+id/txt7"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="8dp"
				android:text="Prettify"
				android:textSize="12sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
